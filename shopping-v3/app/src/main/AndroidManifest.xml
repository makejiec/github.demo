<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shopping_v3">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/taobao"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <service
            android:name=".avtivity.service.MediaPlayerService"
            android:enabled="true"
            android:exported="true"></service>

        <activity android:name=".avtivity.activity.LoginActivity" />
        <activity android:name=".avtivity.activity.RegisterActivity" />
        <activity android:name=".avtivity.activity.MainActivity" />
        <activity android:name=".avtivity.activity.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>